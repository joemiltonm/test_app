{
    "compilerOptions": {
      "target": "ES6",
      "module": "ESNext",
      "jsx": "react-jsx",
      "strict": true,
      "moduleResolution": "node",
      "baseUrl": "./src",
      "paths": {
        "@/*": ["./*"]
      },
      "allowSyntheticDefaultImports": true,
      "esModuleInterop": true,
    },
    "include": ["src/**/*"],
    "exclude": ["node_modules", "dist"],
    
  }
  